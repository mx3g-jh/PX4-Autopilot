#!/bin/sh
#
# board specific sensors init
#------------------------------------------------------------------------------
board_adc start

# SPI1
iim42652 -s -R 22 start

# SPI2
bmi088 -s -b 2 -A -R 29 start
bmi088 -s -b 2 -G -R 29 start

# SPI4
bmp581 start -s

# SPI6

ist8310 start -X -b 3 -R 18

icp201xx -I -a 0x63 -b 4 start
